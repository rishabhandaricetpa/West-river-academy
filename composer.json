{
  "name": "peggywebb/west-river-academy",
  "type": "project",
  "description": "The Dwell at Home app.",
  "license": "proprietary",
  "require": {
    "php": "^7.4|^8.0",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "almasaeed2010/adminlte": "~2.4",
    "ankurk91/laravel-alert": "^2.2",
    "bmatovu/multi-auth": "^10.3",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.12",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.1",
    "stripe/stripe-php": "^7.67",
    "tightenco/ziggy": "^1.0",
    "yajra/laravel-datatables-oracle": "~9.0",
    "yoeunes/toastr": "^1.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "barryvdh/laravel-ide-helper": "^2.8",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^2.16",
    "nunomaduro/collision": "^5.0"
  },
  "replace": {
    "paragonie/random_compat": "*",
    "ralouphie/getallheaders": "*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-intl-idn": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "composer ide-helper"
    ],
    "ide-helper": [
      "@php artisan ide-helper:generate --ansi",
      "@php artisan ide-helper:meta --ansi",
      "@php artisan ide-helper:models -N --ansi"
    ],
    "clear-all": [
      "@php artisan cache:clear --ansi",
      "@php artisan route:clear --ansi",
      "@php artisan view:clear --ansi",
      "@php artisan config:clear --ansi",
      "@php artisan clear-compiled --ansi",
      "composer dump-autoload -o"
    ],
    "cache-all": [
      "@php artisan config:cache --ansi",
      "@php artisan route:cache --ansi",
      "@php artisan view:cache --ansi"
    ],
    "reset": [
      "composer clear-all",
      "composer cache-all",
      "@php artisan queue:restart --ansi"
    ],
    "worker": "@php artisan queue:work --sleep=3 --ansi",
    "format": "./vendor/bin/php-cs-fixer fix --ansi"
  }
}
